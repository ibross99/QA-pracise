LAb 2 


names = []
while len(names) < 3:
    names.append(input("Enter a name: "))

# For loop to print each name with "is cool"
for name in names:
    print(f"{name} is cool")


Password lab 

def check_password():
    # List of common passwords
    common_passwords = ["123456", "password", "123456789"]

    # Ask the user for a password
    user_password = input("Enter a password: ")

    # Check if the password is in the list of common passwords
    if user_password in common_passwords:
        print(f"Use a safer password, '{user_password}' is compromised.")
    else:
        print("Password is safe.")

        check_password()

Lab 2.1

age = int(input("Enter your age: "))

if age >= 18:
    print("You are in category A")
elif age >= 16:
    print("You are in category B")
else:
    print("You are in category C")




Lab 3 

num = 1

while num <= 100:
    square = num ** 2  # Calculate the square of the number
    print(f"Number: {num}, Square: {square}")
    
    # End the loop if the square is greater than 1000
    if square > 1000:
        break
    
    num += 1  # 




